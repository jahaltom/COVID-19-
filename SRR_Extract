import pandas as pd
from pysradb import sraweb



#Open list of Study_Accession numbers.
ids=open("SRPids",'r')
##Make a list to store metadata
metadata=[]
##Load SRA database
db = sraweb.SRAweb()
for i in ids:
    df = db.sra_metadata(i)
    metadata.append(df)

metadata = pd.concat(metadata)
metadata.to_csv("SRP_All_SRR")
